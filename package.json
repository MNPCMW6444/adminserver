{
  "name": "failean-adminserver",
  "version": "1.0.1",
  "scripts": {
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "clean": "rm -rf node_modules package-lock.json dist",
    "clean:p": "rm -rf node_modules package-lock.json",
    "preupdatesimple": "npx npm-check-updates -u",
    "updatesimple": "npm i",
    "preupdate": "npm run clean",
    "update": "npm run updatesimple",
    "prebuild": "npm run version:patch",
    "build": "npx ts-node build.ts",
    "lint": "eslint . --ext .ts",
    "start": "npm run dev",
    "preprod": "npm run update",
    "prod": "npm run build",
    "d:b": "VERSION=$(node -p \"require('./package.json').version\") && docker build --platform amd64 -t 988253048728.dkr.ecr.us-east-1.amazonaws.com/adminserver:$VERSION .",
    "dev": "nodemon",
    "preudev": "npm run update",
    "udev": "npm run dev",
    "pred:b": "rm -f .npmrc"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node ./src"
  },
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.4.4",
    "fs-extra": "^11.1.1",
    "jet-logger": "^1.3.1",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.376.0",
    "@aws-sdk/client-s3": "^3.374.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  }
}
